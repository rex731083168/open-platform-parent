<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="cn.ce.platform_service.apis.dao.IMysqlApiPartDao">
  	<resultMap type="cn.ce.platform_service.apis.entity.ApiPartEntity" 
  		id="apiPartMap">
  		<id column="part_id" property="id"/>
  		<result column="part_type" property="partType"/>
  		<result column="api_id" property="apiId"/>
  		<result column="name" property="name"/>
  		<result column="type" property="type"/>
  		<result column="required" property="required"/>
  		<result column="example" property="example"/>
  		<result column="desc" property="desc"/>
  	</resultMap>
  
  	<insert id="save" parameterType="cn.ce.platform_service.apis.entity.ApiPartEntity">
  		INSERT INTO api_part 
  			(part_id,part_type,api_id,name,type,required,example,desc)
  		VALUES
  			(#{id},#{partType},#{apiId},#{name},#{type},#{required},#{example},#{desc})
  	</insert>
  
  
  
  </mapper>