<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="cn.ce.platform_service.apis.dao.IMysqlApiResultDao">
  	<resultMap type="cn.ce.platform_service.apis.entity.ApiResultEntity" id="apiResultMap">
  		<id column="result_id" property="id"/>
  		<result column="api_id" property="apiId"/>
  		<result column="ret_name" property="retName"/>
  		<result column="ret_type" property="retType"/>
  		<result column="example" property="example"/>
  	</resultMap>
  
  	<insert id="save" parameterType="cn.ce.platform_service.apis.entity.ApiResultEntity">
  		INSERT INTO api_result
  			(result_id,api_id,ret_name,ret_type,example)
  		VALUES
  			(#{id},#{apiId},#{retName},#{retType},#{example})
  	</insert>
  
  
  </mapper>